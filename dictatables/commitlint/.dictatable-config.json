{
  "message": "Setup some git stuff.",
  "actions": [
    {
      "copyFrom": "pre-commit",
      "target": ".git/hooks",
      "chmod": "0755"
    },
    {
      "copyFrom": ".gitmessage",
      "target": "."
    },
    {
      "target": ".gitignore",
      "haveLineContaining": ["/.gitmessage"]
    },
    {
      "target": "this-file-should-not-be-in-git",
      "itShould": "NOT_BE_IN_GIT"
    },
    {
      "target": "this-file-should-not-exist",
      "itShould": "NOT_EXIST"
    }
  ]
}
